submodules/unity3d-store/create_deploys
submodules/unity3d-profile/create_deploys

LEVELUP_FILES=$(<files)

echo "creating unitypackage for SOOMLA LevelUp"

/Applications/Unity/Unity.app/Contents/MacOS/Unity \
  -batchmode \
  -logFile create_unity.log \
  -projectPath $PWD/Soomla/ \
  -exportPackage \
    $LEVELUP_FILES \
  $PWD/soomla-unity3d-levelup.unitypackage \
  -quit
